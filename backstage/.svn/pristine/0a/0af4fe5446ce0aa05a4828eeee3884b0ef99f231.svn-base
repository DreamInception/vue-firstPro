body { background: #f5f5f5; font-size: 12px; font-family: "Microsoft YaHei"; }

a { color: #888888; text-decoration: none; }

ul, li { list-style: none; }

dl, dt, dd, ul, li { padding: 0; margin: 0; }

button { border: none; outline: none; }

.clearfix { *zoom: 1; }
.clearfix:after { content: ""; display: table; clear: both; }

input { outline: none; }

.selectbox { display: inline-block; float: left; }

.maincontent { width: 1200px; margin: 0px auto; }

header { height: 227px; }
header .headBar { height: 80px; background: #fff; }
header .headBar .headCont { width: 1200px; margin: 0px auto; }
header .headBar .headCont .logo { float: left; margin-top: 12px; }
header .headBar .headCont .logo span { float: right; font-size: 30px; color: #333333; margin: 10px 0px 0px 25px; font-weight: bold; }
header .headBar .headCont .title { width: 360px; float: left; font-size: 24px; text-align: center; line-height: 80px; margin-left: 120px; }
header .headBar .headCont .login { width: 112px; height: 77px; float: right; border-top: 3px solid #fff; background: url("../images/icons/down.png") no-repeat left; background-position: 80px 35px; }
header .headBar .headCont .login .user { width: 40px; height: 40px; background: #616b88 url("../images/icons/login.png") no-repeat center; border-radius: 40px; margin: 20px 10px 0px 26px; }
header .headBar .headCont .login:hover { border-top: 3px solid #616b88; background: url("../images/icons/down_hover.png") no-repeat left; background-position: 80px 35px; }
header .headBar .headCont .login .list { position: relative; margin-top: 17px; background: #fff; z-index: 99; display: none; }
header .headBar .headCont .login .list li { font-size: 12px; text-align: center; cursor: pointer; }
header .headBar .headCont .login .list li:hover { background: #dfe5ea; }
header .headBar .headCont .login .list li a { color: #9aacb9; }
header .headBar .headCont .login .list li a:hover { color: #616b88; }
header .headBar .headCont .login .list li .libar { width: 80px; height: 35px; line-height: 35px; margin: 0px auto; border-top: 1px solid #dfe5ea; }
header .headBar .headCont .login .list li:first-child .libar { border-top: none; }
header .headBar .searchBar { width: 1200px; margin: 0px auto; height: 130px; background: url("../images/bg.png") no-repeat; }
header .headBar .searchBar .searchBg { width: 1200px; float: left; margin-top: 45px; }
header .headBar .searchBar .searchBg .search { width: 500px; height: 40px; background: #fff; border-radius: 40px; margin: 0px auto; }
header .headBar .searchBar .searchBg .search .inputCss { float: left; width: 430px; height: 40px; border: 0; padding: 0px 10px; line-height: 40px; display: inline-block; margin-left: 20px; }
header .headBar .searchBar .searchBg .search .btn { width: 28px; height: 40px; float: left; background: url("../images/icons/search.png") no-repeat center; }

footer { width: 100%; height: 60px; position: fixed; background: #84929f; bottom: 0px; }
footer .footBar { width: 1200px; margin: 0px auto; }
footer .footBar p { float: left; text-align: left; line-height: 30px; color: #fff; }
footer .footBar .footList { float: right; }
footer .footBar .footList li { float: left; padding-left: 20px; line-height: 60px; color: #fff; }
footer .footBar .footList li a { color: #fff; }
