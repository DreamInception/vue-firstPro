.table { color: #666; background: #fff; max-width: 100%; margin-bottom: 10px; border: 1px solid #e6e6e6; border-left-width: 0; border-right-width: 0; }
.table.fixed-width { min-width: 1020px; }
.table th, .table td { padding: 10px; vertical-align: middle; text-align: center; border: 1px solid #e6e6e6; }
.table thead th { vertical-align: bottom; background: #d5dbe8; white-space: nowrap; border-bottom: 1px solid #e6e6e6; }
.table thead th, .table thead td { border-bottom-width: 1px; }
.table tbody { border-top: 1px solid #e6e6e6; }
.table a { color: #3498da; }
.table a.disabled { color: #a3a3a3; cursor: not-allowed; }

.table-striped tbody tr:nth-of-type(even) { background-color: #f2f6f9; }

.pad15 { padding: 15px; }

.main { font-size: 12px; color: #666; width: 940px; margin: 15px auto 0; }

.normal_block { padding: 10px 17px; background: #fff; margin-bottom: 17px; }
.normal_block .normal_block_title { line-height: 28px; margin-bottom: 7px; }

.top ul { margin-left: -74px; }
.top ul li { width: 160px; height: 85px; background: #f2f6f9; text-align: center; box-sizing: border-box; padding: 5px 0 0; float: left; margin-left: 74px; }
.top ul li p { padding-bottom: 5px; }
.top ul li p span { font-size: 24px; }

.w_big { font-size: 24px; }

.fl { float: left; }

.order_tips dd { margin: 15px 0; }
.order_tips dd label { float: left; width: 64px; line-height: 27px; }
.order_tips dd .wrap_status li { float: left; padding: 7px 9px; }
.order_tips dd .wrap_status li.cur { background: #d5dbe8; }
.order_tips dd.buyer .selectbox { height: 26px; line-height: 26px; float: left; border-radius: 0; }
.order_tips dd.buyer .selectbox .selectbox_value { font-size: 12px; }
.order_tips dd.buyer .buyer_write { float: left; width: 390px; height: 28px; border: 1px solid #e9e9e9; border-left: none; padding: 0 5px; }
.order_tips dd.buy_time input { width: 143px; height: 28px; border: 1px solid #e9e9e9; box-sizing: border-box; padding: 0 5px 0 12px; background: url(../images/date.png) no-repeat 117px 2px; display: inline-block; }
.order_tips dd.buy_time span { margin: 0 10px; }
.order_tips dd.wrap_btns ul { margin-left: 350px; }
.order_tips dd.wrap_btns ul li { width: 80px; height: 30px; background: #8793b2; color: #fff; font-size: 14px; line-height: 30px; text-align: center; border-radius: 3px; display: block; float: left; cursor: pointer; margin-right: 30px; }

.bottom { padding-bottom: 30px; background: #fff; }

.wrap_table { width: 940px; overflow: scroll; height: auto; min-height: 200px; }
.wrap_table .fixed-width { min-width: 1387px; }
.wrap_table .fixed-width td { line-height: 1.4em; }

.selectbox_list { width: 122px; top: 26px; }

.selectbox_body li { font-size: 12px; }
