#webbar { padding: 10px; background: #f5f5f5; }
#webbar .tab { width: 100%; padding: 20px 15px; background: #fff; margin-bottom: 15px; }
#webbar .tab li { float: left; padding-right: 32px; }
#webbar .tab li a { color: #888888; }
#webbar .tab .active { background: #cfd2db; padding: 6px 15px; border-radius: 5px; }
#webbar .tab .linebar { margin-bottom: 10px; }
#webbar .tab .linebar .lab { font-size: 12px; color: #666666; margin-right: 15px; }
#webbar .tab .linebar .inpclass { width: 348px; height: 30px; padding: 0px 10px; color: #666666; }
#webbar .tab .linebar.mbottom { margin-bottom: 0px; }
#webbar .tab .systitle { color: #333333; }
#webbar .conbar { background: #f2f6f9; padding: 16px; margin-top: 15px; color: #333333; }
#webbar .conbar .checkbar { margin-bottom: 15px; }
#webbar .conbar .checkbar .checkclass { margin-right: 5px; }
#webbar .conbar .subbar { margin-left: 15px; }
#webbar .conbar .subbar .check { margin-right: 6px; }
#webbar .conbar .subbar .checkwidth { width: 250px; }
#webbar .conbar .subbar .checkwidth .masklist { margin-top: 10px; }
#webbar .conbar .subbar .checkwidth .masklist li { float: left; padding-left: 15px; padding-right: 0; }
