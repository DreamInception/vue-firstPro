.table { color: #666; background: #fff; max-width: 100%; margin-bottom: 10px; border: 1px solid #e6e6e6; border-left-width: 0; border-right-width: 0; }
.table.fixed-width { min-width: 1020px; }
.table tr.selected { background: #deeafd; }
.table tr:hover td, .table tr:hover th { background: #deeafd; }
.table th, .table td { padding: 10px; vertical-align: middle; text-align: center; border: 1px solid #e6e6e6; }
.table thead th { vertical-align: bottom; background: #d5dbe8; white-space: nowrap; font-weight: normal; border-bottom: 1px solid #e6e6e6; }
.table thead th, .table thead td { border-bottom-width: 1px; }
.table tbody { border-top: 1px solid #e6e6e6; }
.table a { color: #3498da; }
.table a.disabled { color: #a3a3a3; cursor: not-allowed; }

.table-striped tbody tr:nth-of-type(even) { background-color: #f2f6f9; }
.table-striped tbody tr:nth-of-type(even).selected { background: #deeafd; }

/* Dropdown control */
.selectBox-dropdown { max-width: 120px; max-height: 30px; position: relative; border: solid 1px #d2d2d2; text-decoration: none; text-align: left; color: #666; outline: none; vertical-align: middle; background: #fff; display: inline-block; cursor: default; }

.selectBox-dropdown:focus { border-color: #aaa; }

.selectBox-dropdown:focus .selectBox-arrow { border-color: #aaa; background-image: url(../images/order-up.png); }

.selectBox-dropdown.selectBox-menuShowing { -moz-border-radius-bottomleft: 0; -moz-border-radius-bottomright: 0; -webkit-border-bottom-left-radius: 0; -webkit-border-bottom-right-radius: 0; border-bottom-left-radius: 0; border-bottom-right-radius: 0; }

.selectBox-dropdown .selectBox-label { padding: 5px 15px; display: inline-block; white-space: nowrap; overflow: hidden; }

.selectBox-dropdown .selectBox-arrow { position: absolute; top: 0; right: 0; width: 8px; height: 100%; padding: 0 18px; background: #fff url(../images/order-down.png) 50% 50% no-repeat; }

/* Dropdown menu */
.selectBox-dropdown-menu { position: absolute; z-index: 999; max-height: 200px; min-height: 1em; border: solid 1px #aaa; background: #FFF; -moz-box-shadow: 0 2px 6px rgba(0, 0, 0, 0.2); -webkit-box-shadow: 0 2px 6px rgba(0, 0, 0, 0.2); box-shadow: 0 2px 6px rgba(0, 0, 0, 0.2); overflow: auto; -webkit-overflow-scrolling: touch; }

/* Inline control */
.selectBox-inline { min-width: 150px; outline: none; border: solid 1px #BBB; background: #FFF; display: inline-block; -webkit-border-radius: 4px; -moz-border-radius: 4px; border-radius: 4px; overflow: auto; }

.selectBox-inline:focus { border-color: #666; }

/* Options */
.selectBox-options, .selectBox-options li, .selectBox-options li a { color: #666; list-style: none; display: block; cursor: default; padding: 0; margin: 0; }

.selectBox-options li a { line-height: 1.5; padding: 3px 15px; margin: 3px 1px; white-space: nowrap; overflow: hidden; background: 6px center no-repeat; }

.selectBox-options li.selectBox-hover a { background-color: #EEE; }

.selectBox-options li.selectBox-disabled a { color: #888; background-color: transparent; }

.selectBox-options li.selectBox-selected a { background-color: #EEE; }

.selectBox-options .selectBox-optgroup { color: #666; background: #EEE; font-weight: bold; line-height: 1.5; padding: 0 .3em; white-space: nowrap; }

/* Disabled state */
.selectBox.selectBox-disabled { color: #888 !important; }

.selectBox-dropdown.selectBox-disabled .selectBox-arrow { opacity: .5; border-color: #666; }

.selectBox-inline.selectBox-disabled { color: #888 !important; }

.selectBox-inline.selectBox-disabled .selectBox-options a { background-color: transparent !important; }

.container { color: #666; margin: 16px; background: #fff; }
.container .content { padding: 16px; }
.container .scrollable { overflow-x: auto; }
.container.wide { width: 1020px; }

.content-label { width: 40px; display: block; float: left; line-height: 30px; }

.inputbox { height: 30px; border: 1px solid #d2d2d2; padding: 3px 15px; }

.searchbox { width: 375px; margin-left: 16px; }

.btn, .searchbtn { width: 80px; height: 30px; color: #fff; font-size: 12px; padding: 0px 10px; border: 1px solid #8793b2; border-radius: 8px; line-height: 30px; background-color: #8793b2; text-align: center; vertical-align: middle; display: inline-block; border-radius: 4px; }
.btn.default-btn, .default-btn.searchbtn { background-color: #8793b2; border-color: #8793b2; }

.searchbtn { margin-left: 16px; }

.standard-dropdown { width: 80px; }
.standard-dropdown.wide { width: 120px; }

.btn-group { position: relative; display: inline-block; vertical-align: middle; }

.order-option { width: 80px; border: 1px solid #d2d2d2; height: 30px; display: block; vertical-align: middle; padding: 5px 0 5px 15px; margin-right: 16px; color: #666; }
.order-option .order-group { width: 8px; display: inline-block; height: 20px; float: right; padding-top: 3px; margin-right: 16px; }
.order-option .order-group span { width: 8px; height: 8px; display: inline-block; float: left; }
.order-option .order-group .opder-up { background: url(../images/order-up-active.png) no-repeat; }
.order-option .order-group .opder-down { background: url(../images/order-down.png) no-repeat; }
.order-option .order-group.desc .opder-up { background: url(../images/order-up.png) no-repeat; }
.order-option .order-group.desc .opder-down { background: url(../images/order-down-active.png) no-repeat; }
