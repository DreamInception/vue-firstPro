body { background: #f5f5f5; color: #193249; }

.header { width: 100%; height: 65px; position: absolute; background: #616b88; z-index: 999; left: 0; top: 0; *zoom: 1; }
.header:after { content: ""; display: table; clear: both; }

.footer { width: 100%; height: 57px; position: absolute; z-index: 999; left: 0; bottom: 0; text-align: center; line-height: 57px; }
.footer span { padding: 0 5px; }

.maincontent { width: 100%; margin-top: 65px; background: #f5f5f5; }

.left { width: 230px; position: absolute; background: #fff; color: #666666; z-index: 998; left: 0; top: 80px; overflow: auto; }

.right { margin-left: 240px; background: #fff; top: 14px; position: relative; }

.logo { float: left; padding-top: 15px; }

.headlist { float: right; }
.headlist li { float: left; padding-right: 20px; line-height: 65px; color: #fff; }
.headlist li a { color: #fff; }
.headlist li.quit { width: 64px; height: 65px; background: #414f77 url("../images/nav/quit.png") no-repeat center; cursor: pointer; }

.logorht { height: 30px; }
.logorht li { float: right; text-align: center; padding-right: 10px; }
.logorht li span { float: left; display: block; height: 30px; line-height: 25px; margin: 25px 0; }
.logorht li a { display: block; float: left; line-height: 30px; padding-left: 26px; color: #193249; }
.logorht li a:hover { color: #616b88; }
.logorht li.name a { background-position: 10px 10px; }
.logorht li.name a:hover { background-position: 10px 10px; }
.logorht li.message { margin-left: 10px; }
.logorht li.message a { background-position: 10px 10px; }
.logorht li.message a:hover { background-position: 10px 10px; }
.logorht li.help a { background-position: 10px 10px; }
.logorht li.help a:hover { background-position: 10px 10px; }

.nav .shadow { color: #616b88; }
.nav .shadow a, .nav .shadow span { color: #616b88; }
.nav > li { line-height: 50px; background-repeat: no-repeat !important; background-position: 15px 15px !important; border-bottom: 1px solid #d2d2d2; }
.nav > li:hover { color: #616b88; background: #e3e6ee; }
.nav > li:hover div { color: #616b88; }
.nav > li:hover div a, .nav > li:hover div span { color: #616b88; }
.nav > li span { margin-left: 50px; cursor: pointer; }
.nav > li span:hover { color: #616b88; }
.nav > li li { padding-left: 50px; display: none; background: #616b88; }
.nav > li li a { display: block; }
.nav > li li:hover, .nav > li li.selected { background: #616b88; }
.nav > li li:hover a, .nav > li li.selected a { color: #fff; }

.shown { display: block !important; }

.arrow { width: 10px; height: 50px; background: #616b88; position: absolute; top: 50%; left: -10px; margin-top: -25px; z-index: 999; border-top-right-radius: 3px; border-bottom-right-radius: 3px; line-height: 50px; font-family: "SimSun"; text-align: center; color: #fff; cursor: pointer; }
.arrow:hover { filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=85); opacity: 0.85; }

.switch .left { display: none; }
.switch .right { margin-left: 10px; }

.small_width { max-width: 120px; }

.shown { display: block; }
