@import "compass";
@import "config";
@import "table";
@import "page";
@import "selectbox";

#webbar{
  padding: 10px;
  background:$bg;
  .tab{
    width: 100%;
    padding: 20px 15px;
    background:$whiteColor;
    margin-bottom: 15px;
    li{
      float: left;
      padding-right: 32px;
      a{
        color: $tabColor;
      }
    }
    .active{
      background:$btnBg;
      padding:6px 15px;
      border-radius: 5px;
    }
    .linebar{
      margin-bottom: 10px;
      .lab{
        font-size: 12px;
        color:$subTitleColor;
        margin-right:10px;
      }
      .inpclass{
        width: 348px;
        height: 30px;
        padding: 0px 10px;
        color:$borderColor;
      }
      .inpclass.inpwidth{
        width: 363px;
      }
    }
    .linebar.mbottom{
      margin-bottom: 0px;
      input{
        width: 143px;
        height: 28px;
        border: 1px solid $borderColor;
        box-sizing: border-box;
        padding: 0 5px 0 12px;
        background: url(../images/date.png) no-repeat 117px 2px;
        display: inline-block;
      }
      span{
        margin: 0 10px;
      }
    }
    .systitle{
      color:$titleColor;
    }
  }
  .conbar{
    background:$sysBg;
    padding: 16px;
    margin-top: 15px;
    color: $titleColor;
    .checkbar{
      margin-bottom: 15px;
      .checkclass{
        margin-right: 5px;
      }
    }
    .subbar{
      margin-left: 15px;
      .check{
        margin-right:6px;
      }
      .checkwidth{
        width:250px;
        .masklist{
          margin-top: 10px;
          li{
            float: left;
            padding-left:15px;
            padding-right: 0;
          }
        }
      }
    }
  }
}
.tablebar{
  width: 100%;
  background:$whiteColor;
  padding-bottom: 50px;
}