body { background: #eaecf2; color: #888888; }

.header { width: 100%; height: 65px; position: absolute; background: #616b88; padding: 17px 0 0 30px; top: 0; left: 0; }

.container { clear: both; min-height: 100%; height: auto !important; height: 100%; }

.loginBrdr { width: 400px; height: 400px; position: absolute; top: 50%; left: 50%; margin: -200px 0  0 -200px; }

.loginTitle { font-size: 32px; text-align: center; margin-bottom: 25px; color: #616b88; }

.brdr { background: #fff; border: 1px solid #d2d2d2; padding: 25px 15px; }

.title { font-size: 26px; text-align: center; color: #193249; }

.inputBrdr { width: 338px; border: 2px solid #eee; height: 45px; margin: 20px auto; }
.inputBrdr input[type='text'] { width: 283px; height: 41px; border: 0px solid #d2d2d2; padding: 0px 10px; line-height: 41px; display: inline-block; float: right; }

.username { background: #eee url('data:image/png;base64,') no-repeat 17px center; }

.password { background: #eee url('data:image/png;base64,') no-repeat 13px center; }

.errorBrdr { color: #f26e6e; text-align: right; margin-right: 15px; }

.remember { padding-left: 15px; }
.remember #rememberPwd { margin-right: 10px; }

.submitBtn { width: 339px; height: 40px; color: #fff; font-size: 12px; border: 1px solid #616b88; border-radius: 8px; line-height: 38px; background-color: #616b88; text-align: center; vertical-align: middle; display: inline-block; cursor: pointer; padding: 0; margin: 25px 0 20px  15px; font-size: 20px; line-height: 37px; }
.submitBtn span { padding: 0 10px; }

