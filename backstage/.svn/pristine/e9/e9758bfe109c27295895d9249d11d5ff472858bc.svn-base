body { padding: 0 16px; min-width: 1024px; }

.title { line-height: 40px; }

.resocProp { width: 100%; padding: 16px; background: #E0E5EB; }
.resocProp .resocImg { width: 200px; height: 148px; line-height: 148px; background: #616A87; margin-right: 16px; float: left; text-align: center; }
.resocProp .resocImg a { width: 100px; height: 30px; color: #616b88; font-size: 12px; padding: 0px 10px; border: 1px solid #616b88; border-radius: 8px; line-height: 30px; background-color: #fff; text-align: center; vertical-align: middle; display: inline-block; -moz-border-radius: 3px; -webkit-border-radius: 3px; border-radius: 3px; padding: 0; font-size: 14px; }
.resocProp dl dt { margin: 6px 0 10px 0; }
.resocProp dl dd { margin-bottom: 10px; font-size: 12px; }

.resocDetail { width: 60%; padding-right: 16px; margin-bottom: 15px; }
.resocDetail > a { float: left; width: 78px; height: 28px; color: #193249; font-size: 12px; padding: 0px 10px; border: 1px solid #d2d2d2; line-height: 28px; background: #fff; text-align: center; vertical-align: middle; display: inline-block; font-size: 14px; }
.resocDetail > a:first-child { border-right: none 0; }
.resocDetail > a.active { background: #616b88; color: #fff; }
.resocDetail ul { float: right; font-size: 12px; }
.resocDetail ul li { float: left; line-height: 28px; margin-right: 18px; }
.resocDetail ul li a { color: #3899DA; text-decoration: underline; }

.resocVideo { width: 100%; }
.resocVideo .videoboxSm { float: left; width: 60%; padding-right: 16px; }
.resocVideo .videoboxSm div.video { width: 100%; height: 490px; background: #947988; }
.resocVideo .videoboxLg { float: right; width: 40%; }
.resocVideo .videoboxLg ul.videoDetail { background: #E0E5EB; padding: 8px 16px; margin-bottom: 16px; }
.resocVideo .videoboxLg ul.videoDetail li { margin-bottom: 16px; }
.resocVideo .videoboxLg ul.videoDetail li div { display: inline-block; }
.resocVideo .videoboxLg ul.videoDetail li div.list { width: 60%; float: left; }
.resocVideo .videoboxLg ul.videoDetail li div.time { margin-right: 70px; }
.resocVideo .videoboxLg ul.videoDetail li span { display: inline-block; width: 70px; }
.resocVideo .videoboxLg ul.videoDetail li span.listTil { float: left; }
.resocVideo .videoboxLg ul.videoDetail li span.mR { width: auto; margin-right: 28px; }
.resocVideo .videoboxLg ul.videoDetail li span.mRsm { width: auto; margin-right: 14px; }
.resocVideo .videoboxLg ul.videoDetail li input[type="radio"] { margin: 0 6px 0 2px; }
.resocVideo .videoboxLg ul.videoDetail li ul.list { width: 70%; float: left; }
.resocVideo .videoboxLg ul.videoDetail li ul.list li:first-child { margin-top: 0; }
.resocVideo .videoboxLg ul.videoDetail li ul.list li span { width: 50px; }
.resocVideo .videoboxLg ul.videoDetail li ul.list li input[type="radio"] { margin: 0 6px; }
.resocVideo .videoboxLg ul.videoDetail li ul.list li input[type="text"] { width: 92px; height: 28px; border: 0; padding: 0px 10px; line-height: 28px; display: inline-block; border: 1px solid #d2d2d2; margin-right: 6px; }
.resocVideo .videoboxLg ul.videoDetail li ul.list li div.mL { display: inline-block; margin: 16px 0 0 89px; }
.resocVideo .videoboxLg ul.videoDetail li ul.list li a.setTime, .resocVideo .videoboxLg ul.videoDetail li ul.list li span.setTime { width: auto; padding-left: 24px; color: #3899da; background: url("../images/settime.png") no-repeat left center; }
.resocVideo .videoboxLg ul.videoDetail li ul.list li span.setTime { width: auto; font-size: 12px; color: #193249; }
.resocVideo .videoboxLg ul.videoDetail li label { display: inline-block; width: 60px; }

div.btn { width: 60%; padding-right: 16px; text-align: center; margin-bottom: 16px; }
div.btn button { width: 80px; height: 30px; color: #616b88; font-size: 12px; padding: 0px 10px; border: 1px solid #616b88; border-radius: 8px; line-height: 30px; background-color: #616b88; text-align: center; vertical-align: middle; display: inline-block; -moz-border-radius: 3px; -webkit-border-radius: 3px; border-radius: 3px; color: #fff; margin: 0 5px; padding: 0; }

.commentDtl div.comdtlChange { margin-bottom: 20px; }
.commentDtl div.comdtlChange a { width: 118px; height: 28px; color: #193249; font-size: 12px; padding: 0px 10px; border: 1px solid #d2d2d2; line-height: 28px; background: #fff; text-align: center; vertical-align: middle; display: inline-block; font-size: 14px; float: left; }
.commentDtl div.comdtlChange a:first-child { border-right: none 0; }
.commentDtl div.comdtlChange a.active { background: #616b88; color: #fff; }
.commentDtl ul li { margin-bottom: 10px; }
.commentDtl ul li span.point { line-height: 30px; padding-right: 110px; margin-right: 10px; background: url("../images/xx.png") no-repeat right center; }
.commentDtl ul li span.score { width: 43px; height: 28px; color: #616b88; font-size: 12px; padding: 0px 10px; border: 1px solid #d2d2d2; line-height: 28px; background: #fff; text-align: center; vertical-align: middle; display: inline-block; padding: 0; }
.commentDtl table { text-align: center; margin-bottom: 20px; }
.commentDtl table tr th, .commentDtl table tr td { padding: 10px 0; border-right: 1px solid #d2d2d2; border-bottom: 1px solid #d2d2d2; }
.commentDtl table tr th:last-child, .commentDtl table tr td:last-child { border-right: none 0; }
.commentDtl table tr th { background: #8893B3; }
.commentDtl table tr td { background: #fff; }
.commentDtl table tr td a { color: #3899DA; }
.commentDtl table tr td a.active { color: #999; }
